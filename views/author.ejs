                        <p>
                            <%=user.skills[2]%>
                        </p>
                        <p>80%</p>
                    </div>
                    <div><img src="menu_icon/80.png" class="line-icon" /></div>
                    <div class="row2">
                        <p>
                            <%=user.skills[3]%>
                        </p>
                        <p>80%</p>
                    </div>
                    <div><img src="menu_icon/80.png" class="line-icon" /></div>
                </div>
                <div class="left-margin"></div>
                <div class="left5">
                    <h1 class="h1">Extra Skills</h1>
                    <div class="left5-col">
                        <img src="menu_icon/Icons.png" class="icon5" alt="" />
